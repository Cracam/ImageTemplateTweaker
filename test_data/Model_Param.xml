<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : Carte Model V3
    V1 and V2 doen't exist on the soft
    Recto pour la partie avec les consos
    verso pour la partie avec les cases nom/prenom
-->
<Model name="Carte Foyer V3">
		<Informations>
			<DefaultDesign name='Carte_Classique'/>
			<Description Description="Ce modèle de carte est le modèle 19 conso + 2 consos vertes de l'association FOYER ENSE3"/>
		</Informations>


		<Output name="Recto" size_x="88" size_y="58" ><!-- Verso de la carte (côte nom prenom) -->
				<Layer name="test" tab_name="Commun">
					<pos pos_x="0" pos_y="0"/>
					<size size_x="88" size_y="58"/>
					<Generator>
						<G_Custom_Image>
						</G_Custom_Image>
					</Generator>
					<Transformers>
						<T_Custom_Color>
						</T_Custom_Color>
					</Transformers>
				</Layer>

				<Layer name="test___2" tab_name="Commun">
					<pos pos_x="0" pos_y="0"/>
					<size size_x="88" size_y="58"/>
					<Generator>
						<G_Custom_Image>
						</G_Custom_Image>
					</Generator>
					<Transformers>
						<T_Mouvable_Image>
							<OffSet X_MaxOffset="30.0" X_MinOffset="-3.0" Y_MaxOffset="30.0" Y_MinOffset="-14.5"/>
						</T_Mouvable_Image>
					</Transformers>
				</Layer>


				<Layer name="test FIXED Image" tab_name="Commun">
					<pos pos_x="0" pos_y="0"/>
					<size size_x="88" size_y="40"/>
					<Generator>
						<G_Fixed_Image>
							<Image image_name="RECTO_arrierePlan.png"/>
						</G_Fixed_Image>
					</Generator>
					<Transformers>
					</Transformers>
				</Layer>
				

				<Layer name="test Custom TEXT" tab_name="Commun">
					<pos pos_x="40" pos_y="15"/>
					<size size_x="68" size_y="18"/>
					<Generator>
						<G_Custom_Text>
							<Text TextHeightFactor="3.0" TextSizeMin="2.5" TextSizeMax="15"/>
						</G_Custom_Text>
					</Generator>
					<Transformers>
						<T_Custom_Color>
						</T_Custom_Color>
					</Transformers>
				</Layer>	

				
				<Layer name="test Custom TEXT" tab_name="Commun">
					<pos pos_x="40" pos_y="15"/>
					<size size_x="68" size_y="18"/>
					<Generator>
						<G_Fixed_Text_Custom_Color_Custom_Style>
							<Text TextHeightFactor="3.0" TextSizeMin="2.5" TextSizeMax="15" Text="Bla __ Blaé"/>
						</G_Fixed_Text_Custom_Color_Custom_Style>
					</Generator>
					<Transformers>
						<T_Custom_Color>
						</T_Custom_Color>
					</Transformers>
				</Layer>


				<Layer name="test rdn disp" tab_name="disp">
					<pos pos_x="40" pos_y="15"/>
					<size size_x="68" size_y="14"/>
					<Generator>
						<G_Random_Image_Dispersion>
							<Size maxXSize="5.0" minXSize="2.0" maxYSize="3.0" minYSize="1.0"/>
							<Interval maxInterval="10.0" minInterval="4.0"/>
							<SubLayer>
								<Generator>
								<G_Fixed_Image>
									<Image image_name="graine.png"/>
								</G_Fixed_Image>
								</Generator>
								<Transformers>
									<T_Custom_Color>
									</T_Custom_Color>
								</Transformers>
							</SubLayer>
						</G_Random_Image_Dispersion>
					</Generator>
					<Transformers>
						<T_Mouvable_Image>
							<OffSet X_MaxOffset="30.0" X_MinOffset="-3.0" Y_MaxOffset="30.0" Y_MinOffset="-14.5"/>
						</T_Mouvable_Image>
					</Transformers>
				</Layer>



				<Layer name="test rdn allocV2" tab_name="dispAl">
					<pos pos_x="0" pos_y="5"/>
					<size size_x="88" size_y="54"/>
					<Generator>
						<G_Random_Image_Allocation>
							<Param minNumber="1" maxNumber="10" defaultNumber="5"/>
							
							<PosSizes>
								<PosSize pos_x="10" pos_y="15" size_x="18.5" size_y="18.5"/>
								<PosSize pos_x="30" pos_y="20" size_x="14.5" size_y="14.5"/>
								<PosSize pos_x="10" pos_y="35" size_x="14.5" size_y="14.5"/>
								<PosSize pos_x="30" pos_y="40" size_x="10.5" size_y="10.5"/>
							</PosSizes>

							<SubLayer name="Consos">
								<Common_Componments>
									<Generator>
										<G_Fixed_Image>
											<Image image_name="graine.png"/>
										</G_Fixed_Image>
									</Generator>
									<Transformers>
										<T_Custom_Color>
										</T_Custom_Color>
									</Transformers>

								</Common_Componments>
								
								<Transformers>
									<T_Custom_Color>
									</T_Custom_Color>
								</Transformers>
							</SubLayer>

						</G_Random_Image_Allocation>
					</Generator>
					<Transformers>
						<T_Mouvable_Image>
							<OffSet X_MaxOffset="30.0" X_MinOffset="-3.0" Y_MaxOffset="30.0" Y_MinOffset="-14.5"/>
						</T_Mouvable_Image>
					</Transformers>
				</Layer>
				


				<Layer name="test rdn alloc" tab_name="dispAl">
					<pos pos_x="0" pos_y="5"/>
					<size size_x="88" size_y="54"/>
					<Generator>
						<G_Random_Image_Allocation>
							<Param minNumber="1" maxNumber="10" defaultNumber="5"/>
							
							<PosSizes>
								<PosSize pos_x="10" pos_y="15" size_x="18.5" size_y="18.5"/>
								<PosSize pos_x="30" pos_y="20" size_x="14.5" size_y="14.5"/>
								<PosSize pos_x="10" pos_y="35" size_x="14.5" size_y="14.5"/>
								<PosSize pos_x="30" pos_y="40" size_x="10.5" size_y="10.5"/>
							</PosSizes>

							<SubLayer name="Consos">
								<Common_Componments>

								</Common_Componments>
								<Generator>
									<G_Fixed_Image>
										<Image image_name="graine.png"/>
									</G_Fixed_Image>
								</Generator>
								<Transformers>
									<T_Custom_Color>
									</T_Custom_Color>
								</Transformers>
							</SubLayer>

						</G_Random_Image_Allocation>
					</Generator>
					<Transformers>
						<T_Mouvable_Image>
							<OffSet X_MaxOffset="30.0" X_MinOffset="-3.0" Y_MaxOffset="30.0" Y_MinOffset="-14.5"/>
						</T_Mouvable_Image>
					</Transformers>
				</Layer>
				

		</Output>	

		<Output name="Verso" size_x="88" size_y="58" ><!-- Verso de la carte (côte nom prenom) -->

				<Layer name="test" tab_name="Commun">
					<pos pos_x="0" pos_y="0"/>
					<size size_x="88" size_y="58"/>
					<Generator>
						<G_Custom_Image>
						</G_Custom_Image>
					</Generator>
					<Transformers>
