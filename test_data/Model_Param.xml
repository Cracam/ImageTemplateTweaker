<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : Carte Model V3
    V1 and V2 doen't exist on the soft
    Recto pour la partie avec les consos
    verso pour la partie avec les cases nom/prenom
-->
<Model name="Carte Foyer V3">
		<Informations>
			<DefaultDesign name='Carte_Classique'/>
			<Description Description="Ce modèle de carte est le modèle 19 conso + 2 consos vertes de l'association FOYER ENSE3"/>
		</Informations>


		<Output name="Recto" size_x="88" size_y="58" ><!-- Verso de la carte (côte nom prenom) -->

				<Fixed_Image name="Fond">
					<pos pos_x="0" pos_y="0" />
					<size size_x="88" size_y="58" />
					<Param>
						<Image  image_name="fond test.png"/>
					</Param>
				</Fixed_Image>


				<img_color name="Bordure">
					<pos pos_x="0" pos_y="0"/>
					<size size_x="88" size_y="58"/>
					<Param>
						<Image image_name="Verso_Bordure.png"/>
						<Color ID="1"/>
					</Param>
				</img_color>


				<Custom_Image name="Tasses">
					<pos pos_x="15" pos_y="5"/>
					<size size_x="40" size_y="20"/>
					<Param>
					</Param>
				</Custom_Image>


				<img_custom_shape name="consos Verte"> <!-- pas utilisé dans le modèle V3 --> 
					<pos pos_x="25" pos_y="5"/>
					<size size_x="10" size_y="10"/>
					<Param>
						<Color color="#23EF34"/>
					</Param>
				</img_custom_shape>


				<Custom_Image name="Tasses_2">
					<pos pos_x="15" pos_y="5"/>
					<size size_x="40" size_y="20"/>
					<Param>
					</Param>
				</Custom_Image>

		</Output>








		<Output name="Verso" size_x="88" size_y="58" ><!-- Recto de la carte (côté consos)--> 
				<Fixed_Image name="Fond">
					<pos pos_x="0" pos_y="0"/>
					<size size_x="88" size_y="58"/>
					<Param>
						<Image image_name="test.png"/>
					</Param>
				</Fixed_Image>


				<img_color name="Bordure">
					<pos pos_x="0" pos_y="0"/>
					<size size_x="88" size_y="58"/>
					<Param>
						<Image image_name="Verso_Bordure.png"/>
						<Color ID="1"/>
					</Param>
				</img_color>


				<Custom_Image name="Tasses">
					<pos pos_x="15" pos_y="5"/>
					<size size_x="40" size_y="20"/>
					<Param>
					</Param>
				</Custom_Image>


				<img_custom_shape name="consos Verte"> <!-- pas utilisé dans le modèle V3 --> 
					<pos pos_x="25" pos_y="5"/>
					<size size_x="10" size_y="10"/>
					<Param>
						<Color color="#23EF34"/>
					</Param>
				</img_custom_shape>


				<img_Consos>
					<size size_x="14.5" size_y="14.5"/>
				</img_Consos>


				<img_Items>
					<size size_x="14.5" size_y="14.5"/>
				</img_Items>


				<Custom_Image name="Tasses">
					<pos pos_x="15" pos_y="5"/>
					<size size_x="40" size_y="20"/>
					<Param>
					</Param>
				</Custom_Image>

		</Output>



</Model>
	
